  <!-- mvn help:active-profiles -->
  <!-- mvn help:effective-pom -->
  <profiles>
    <profile>
      <id>useWNHOMEDataForTest</id>
      <activation>
        <property>
          <name>useWNHOMEDataForTest</name>
        </property>
      </activation>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-surefire-plugin</artifactId>
            <configuration>
              <!-- Required to pass this system property through to the unit tests -->
              <systemProperties>
                <property>
                  <!-- NOTE: will be slower since can't take advantage of Bloom filters! -->
                  <name>WNHOME</name>
                  <!--suppress MavenModelInspection -->
                  <value>${WNHOME}</value>
                  <!-- could also pass <value> ${env.WNHOME} ? -->
                </property>
              </systemProperties>
              <!--
              <file>
                <exists>${env.WNHOME}</exists>
              <file>
              -->
              <!-- also have possibility of using <environmentVariables> -->
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
